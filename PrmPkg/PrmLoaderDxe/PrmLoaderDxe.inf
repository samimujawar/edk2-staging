[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = PrmLoaderDxe
  FILE_GUID                      = 226A500A-E14F-414A-A956-40E5762D3D1E
  MODULE_TYPE                    = DXE_DRIVER
  VERSION_STRING                 = 1.0
  ENTRY_POINT                    = PrmLoaderEntryPoint

[Sources]
  PrmAcpiTable.h
  PrmLoader.h
  PrmLoaderDxe.c

[Packages]
  MdePkg/MdePkg.dec
  MdeModulePkg/MdeModulePkg.dec
  PrmPkg/PrmPkg.dec

[Guids]
  gEfiEndOfDxeEventGroupGuid

[LibraryClasses]
